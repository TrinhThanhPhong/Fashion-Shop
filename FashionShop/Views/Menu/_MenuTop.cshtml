@model IEnumerable<FashionShop.Models.EF.Category>

@if (Model != null && Model.Any())
{
<ul class="navbar_menu">
    @foreach (var item in Model)
    {
        if(item.Alias.Equals("trang-chu"))
        {
            item.Alias = "";
        }
        if(item.Alias.Equals("tin-tuc"))
        {
            item.Alias = "news";
        }
        if (item.Alias.Equals("san-pham"))
        {
            item.Alias = "products";
        }
        if (item.Alias.Equals("lien-he"))
        {
            item.Alias = "contact";
        }
        <li><a href="/@item.Alias">@item.Title</a></li>
    }

    @*<li><a href="#">shop</a></li>
    <li><a href="#">promotion</a></li>
    <li><a href="#">pages</a></li>
    <li><a href="#">blog</a></li>
    <li><a href="contact.html">contact</a></li>*@
</ul>
}